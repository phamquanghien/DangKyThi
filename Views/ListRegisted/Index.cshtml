@model IEnumerable<QuanLyCaThi.Models.ListRegisted>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Subject)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExamTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subject.SubjectID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.StudentID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExamTime.ExamTimeID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ListRegistedID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ListRegistedID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ListRegistedID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
